{
  "entry": "app/assets/*.js",
  "theme": {
    "@primary-color": "#dc6aac",
    "@link-color": "#dc6aac",
    "@border-radius-base": "2px",
    "@font-size-base": "16px",
    "@line-height-base": "1.2"
  },
  "extraBabelPlugins": [
    [ "import", { "libraryName": "antd", "style": true } ]
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    }
  },
  "outputPath": "app/public",
  "hash": true,
  "manifest": {
    "fileName": "../../config/manifest.json"
  }
}
